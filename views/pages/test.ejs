<h1>Alumnos:</h1>
<table id="alumno" class="table table-striped table-hover" style="width: 50%; margin:auto;">

  <thead>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
  </tr>
  </thead>
  <tbody>
    <% for(var i = 0; i < a.length; i++) { %>
  <tr>
    <td><%= a[i].nombre %></td>
    <td><%= a[i].apellido %></td>
    <td><a href="/Alumno/<%= a[i].id %>" class="btn-info btn">
        <i class="fas fa-eye"></i>
      </a>

    </td>
  </tr>
  <% } %>
  </tbody>
</table>

